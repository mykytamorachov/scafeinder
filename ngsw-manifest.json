{
  "routing": {
    "routes": {
      "/": {
        "match": true
      },
      "/login": {
        "match": "exact"
      },
      "/profile": {
        "match": "exact"
      },
      "/register": {
        "match": "prefix"
      },
      "/restaurants": {
        "match": "prefix"
      }
    }
  },
  "external": {
     "urls": [{
       "url": "http://localhost:3000/cafes"
       }]
  },
  "dynamic": {
    "group": [{
        "name": "cafe",
        "urls": {
          "http://localhost:3000/cafe": {
            "match": "prefix"
          }
        },
        "cache": {
          "optimizeFor": "performance",
          "strategy": "lru",
          "maxAgeMs": 3600000,
          "maxEntries": 2
        }
      },
      {
        "name": "openstreetmap",
        "urls": {
          "https://openstreetmap.org": {
            "match": "prefix"
          }
        },
        "cache": {
          "optimizeFor": "performance",
          "strategy": "lru",
          "maxAgeMs": 3600000,
          "maxEntries": 2,
          "networkTimeoutMs": 1000
        }
      }
    ]
  } 
}
